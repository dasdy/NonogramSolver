<Window x:Class="NonogramSolver.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:NonogramSolver"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800"
        SizeToContent="WidthAndHeight">
    <StackPanel>
        <StackPanel Orientation="Horizontal">
            <local:NonogramView x:Name="filledNonogram">

            </local:NonogramView>
            <local:NonogramView x:Name="emptyNonogram">

            </local:NonogramView>
        </StackPanel>
        <TextBlock Text="sample text" Name="statusText">

        </TextBlock>
    </StackPanel>
</Window>
