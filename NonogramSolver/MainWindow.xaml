<Window x:Class="NonogramSolver.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:NonogramSolver"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="1000"
        SizeToContent="WidthAndHeight">
    <WrapPanel Orientation="Horizontal">
        <local:NonogramView x:Name="filledNonogram">

        </local:NonogramView>
        <local:NonogramView x:Name="emptyNonogram">

        </local:NonogramView>
        <local:NonogramView x:Name="filledFully">

        </local:NonogramView>
        <local:NonogramView x:Name="acSolved">

        </local:NonogramView>
        <TextBlock Text="sample text" Name="statusText">

        </TextBlock>
    </WrapPanel>
</Window>
